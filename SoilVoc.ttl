she:PoreAbundance a skos:Concept ;
    skos:altLabel "pores abundance"@en ;
    skos:broader she:SoilPores ;
    skos:definition [ dcterms:source glosis_lh:poresAbundanceProperty ;
            schema:text "The Pores Abundance describes the quantity of soil pores, ranging from few to many. It influences water infiltration, root growth, and soil aeration. Pore abundance is crucial for assessing soil health and productivity. Source: Soil Survey Manual (USDA, 2017)."@en ] ;
    skos:exactMatch glosis_lh:poresAbundanceProperty ;
    skos:inScheme <https://soilwise-he.github.io/soil-health> ;
    skos:prefLabel "pore abundance"@en .

she:PoreSize a skos:Concept ;
    skos:altLabel "pores size"@en ;
    skos:broader she:SoilPores ;
    skos:definition [ dcterms:source glosis_lh:poresSizeProperty ;
            schema:text "The Pores Size categorizes soil pores by their diameter, such as macro-pores, meso-pores, and micro-pores. This property determines water retention, air movement, and root penetration in soil. Source: Brady, N.C., & Weil, R.R. (2008). The Nature and Properties of Soils."@en ] ;
    skos:exactMatch glosis_lh:poresSizeProperty ;
    skos:inScheme <https://soilwise-he.github.io/soil-health> ;

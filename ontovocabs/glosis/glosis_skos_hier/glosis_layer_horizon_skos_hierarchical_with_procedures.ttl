she:StructureGrade a skos:Concept ;
    skos:broader she:SoilStructure ;
    skos:definition "The Structure Grade assesses the development and stability of soil aggregates, classified as weak, moderate, or strong. It reflects soil tilth, aeration, and resistance to erosion. Source: Soil Survey Staff (2014). Keys to Soil Taxonomy."@en ;
    skos:exactMatch glosis_lh:structureGradeProperty ;
    skos:prefLabel "structure grade"@en .

she:StructureSize a skos:Concept ;
